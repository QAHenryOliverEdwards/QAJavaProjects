package com.qa.main;

public class Flowchart {

    public void flow1() {
        for (int A = 100; A < 200; A++){
            System.out.println("A");
        }
    }

    public void flow2() {
        for (int A = 100; A > 200; A++) {
            if (A%2 == 0) {
                System.out.println("-");
            }
            else {
                System.out.println("*");
            }
        }
    }

    public void flow1to10() {
        for (int i = 1; i <= 10; i++) {
            for (int j = 1; j <= 10; j++){
                System.out.println(j);
            }
        }
    }

    public void flow1to10PrintValueOfNumber() {
        for (int i = 1; i <= 10; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.println(i);
            }
        }
    }
}
